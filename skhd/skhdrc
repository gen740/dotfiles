cmd + ctrl + shift + alt - a : yabai -m space --focus 1
cmd + ctrl + shift + alt - o : yabai -m space --focus 2
cmd + ctrl + shift + alt - e : yabai -m space --focus 3
cmd + ctrl + shift + alt - u : yabai -m space --focus 4
cmd + ctrl + shift + alt - i : yabai -m space --focus 5

ctrl + shift + alt - 0x27 : yabai -m window --space 1 # quate
ctrl + shift + alt - 0x2B : yabai -m window --space 2 # comma
ctrl + shift + alt - 0x2F : yabai -m window --space 3 # period
ctrl + shift + alt - p : yabai -m window --space 4
ctrl + shift + alt - y : yabai -m window --space 5

ctrl + shift + alt - 0x3B : yabai -m window --toggle zoom-parent

ctrl + shift + alt - b : yabai -m space --layout bsp
ctrl + shift + alt - s : yabai -m space --layout stack
ctrl + shift + alt - f : yabai -m space --layout float

ctrl + shift + alt - f : yabai -m window --toggle float

ctrl + shift + alt - h : yabai -m window --swap west
ctrl + shift + alt - n : yabai -m window --swap east
ctrl + shift + alt - c : yabai -m window --swap north
ctrl + shift + alt - w : yabai -m window --swap east

cmd + ctrl + shift + alt - h : yabai -m window --focus west
cmd + ctrl + shift + alt - n : yabai -m window --focus east
cmd + ctrl + shift + alt - c : yabai -m window --focus north
cmd + ctrl + shift + alt - w : yabai -m window --focus east

