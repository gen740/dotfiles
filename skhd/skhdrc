ctrl - 1 : yabai -m space --focus 1
ctrl - 2 : yabai -m space --focus 2
ctrl - 3 : yabai -m space --focus 3
ctrl - 4 : yabai -m space --focus 4

ctrl + shift - 1 : yabai -m window --focus 1
ctrl + shift - 2 : yabai -m window --focus 2
ctrl + shift - 3 : yabai -m window --focus 3
ctrl + shift - 4 : yabai -m window --focus 4
