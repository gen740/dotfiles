{
    "include": [
        "src"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "src/experimental",
        "src/typestubs"
    ],
    "ignore": [
        "src/oldstuff"
    ],
    "defineConstant": {
        "DEBUG": true
    },
    "stubPath": "src/stubs",
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "pythonVersion": "3.10",
    "pythonPlatform": "Linux",
    "strict": "Strict",
    "executionEnvironments": [
        {
            "root": "src/web",
            "pythonVersion": "3.5",
            "pythonPlatform": "Windows",
            "extraPaths": [
                "src/service_libs"
            ]
        },
        {
            "root": "src/sdk",
            "pythonVersion": "3.0",
            "extraPaths": [
                "src/backend"
            ]
        },
        {
            "root": "src/tests",
            "extraPaths": [
                "src/tests/e2e",
                "src/sdk"
            ]
        },
        {
            "root": "src"
        }
    ]
}
